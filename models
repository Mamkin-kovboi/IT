class Currency:
    def __init__(self, currency_id: int, name: str):
        self.currency_id = currency_id
        self.name = name

class CurrencyPrice:
    def __init__(self, currency_pair_id: int, price: float, source: str, datetime: str):
        self.currency_pair_id = currency_pair_id
        self.price = price
        self.source = source
        self.datetime = datetime

class Exchanger:
    def __init__(self, exchanger_id: int, name: str, api_url: str):
        self.exchanger_id = exchanger_id
        self.name = name
        self.api_url = api_url

class CurrencyPair:
    def __init__(self, currency_pair_id: int, currency_from_id: int, currency_to_id: int, exchanger_id: int):
        self.currency_pair_id = currency_pair_id
        self.currency_from_id = currency_from_id
        self.currency_to_id = currency_to_id
        self.exchanger_id = exchanger_id


